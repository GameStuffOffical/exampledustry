// Example of a wall that conducts electricity
name: Silicon Wall
description: A costly wall that conducts electricity.
type: wall
size: 1
// Health will scale depending on size of the block.
scaledHealth: 100

// Can power nodes connect to this block.
connectedPower: false
// Can block conduct power?
conductive: true

solid: true

// Resorces needed to build block. 
requirements: [
  silicon/50
  copper/5
]

// Category block will show up in the block slection menue.
category: walls
